import random
numerosJogador1 = []
numerosJogador2 = []
d6 = [1,2,3,4,5,6]
for a in range(3):
	sort1 = random.choice(d6)
	numerosJogador1.append(sort1)
for b in range(3):
	sort2 = random.choice(d6)
	numerosJogador2.append(sort2)
soma1 = sum(numerosJogador1)
soma2 = sum(numerosJogador2)
if soma1 > soma2:
	print("O jogador 1 venceu!, os dados foram:" + str(numerosJogador1))
	print("Os números do jogador 2 foram:" + str(numerosJogador2))
else:
	print("O jogador 2 venceu!, os dados foram:" + str(numerosJogador2))
	print("Os números do jogador 1 foram:" + str(numerosJogador1))
